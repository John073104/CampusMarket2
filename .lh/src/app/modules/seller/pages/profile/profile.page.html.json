            "date": 1763384538027,
            "name": "Commit-0",
            "content": "<ion-header [translucent]=\"true\">\r\n  <ion-toolbar color=\"primary\">\r\n    <ion-buttons slot=\"start\">\r\n      <ion-back-button defaultHref=\"/seller/dashboard\"></ion-back-button>\r\n    </ion-buttons>\r\n    <ion-title>My Profile</ion-title>\r\n    <ion-buttons slot=\"end\">\r\n      @if (!editMode) {\r\n        <ion-button (click)=\"toggleEditMode()\">\r\n          <ion-icon slot=\"icon-only\" name=\"create\"></ion-icon>\r\n        </ion-button>\r\n      }\r\n    </ion-buttons>\r\n  </ion-toolbar>\r\n</ion-header>\r\n\r\n<ion-content [fullscreen]=\"true\">\r\n  @if (user) {\r\n    <div class=\"profile-container\">\r\n      <!-- Profile Header -->\r\n      <div class=\"profile-header\">\r\n        <ion-avatar class=\"large-avatar\">\r\n          @if (user.profileImage) {\r\n            <img [src]=\"user.profileImage\" [alt]=\"user.name\" (error)=\"onImageError($event)\" />\r\n          } @else {\r\n            <div class=\"avatar-placeholder\">\r\n              <ion-icon name=\"person\" size=\"large\"></ion-icon>\r\n            </div>\r\n          }\r\n        </ion-avatar>\r\n        <h2>{{ user.name || 'Seller' }}</h2>\r\n        <p class=\"role-badge\">\r\n          <ion-badge color=\"success\">Seller</ion-badge>\r\n        </p>\r\n      </div>\r\n\r\n      <!-- Profile Info Card -->\r\n      <ion-card>\r\n        <ion-card-header>\r\n          <ion-card-title>Profile Information</ion-card-title>\r\n        </ion-card-header>\r\n\r\n        <ion-card-content>\r\n          @if (!editMode) {\r\n            <!-- View Mode -->\r\n            <div class=\"info-item\">\r\n              <ion-icon name=\"mail\" color=\"primary\"></ion-icon>\r\n              <div>\r\n                <p class=\"label\">Email</p>\r\n                <p class=\"value\">{{ user.email }}</p>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"info-item\">\r\n              <ion-icon name=\"person\" color=\"primary\"></ion-icon>\r\n              <div>\r\n                <p class=\"label\">Name</p>\r\n                <p class=\"value\">{{ user.name || 'Not set' }}</p>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"info-item\">\r\n              <ion-icon name=\"call\" color=\"primary\"></ion-icon>\r\n              <div>\r\n                <p class=\"label\">Phone</p>\r\n                <p class=\"value\">{{ user.phone || 'Not set' }}</p>\r\n              </div>\r\n            </div>\r\n          } @else {\r\n            <!-- Edit Mode -->\r\n            <form #profileForm=\"ngForm\">\r\n              <ion-item>\r\n                <ion-label position=\"stacked\">Name *</ion-label>\r\n                <ion-input \r\n                  [(ngModel)]=\"profileData.name\"\r\n                  name=\"name\"\r\n                  type=\"text\"\r\n                  placeholder=\"Enter your name\"\r\n                  required>\r\n                </ion-input>\r\n              </ion-item>\r\n\r\n              <ion-item>\r\n                <ion-label position=\"stacked\">Phone *</ion-label>\r\n                <ion-input \r\n                  [(ngModel)]=\"profileData.phone\"\r\n                  name=\"phone\"\r\n                  type=\"tel\"\r\n                  placeholder=\"Enter phone number\"\r\n                  required>\r\n                </ion-input>\r\n              </ion-item>\r\n\r\n              <div class=\"button-group\">\r\n                <ion-button \r\n                  expand=\"block\" \r\n                  (click)=\"saveProfile()\"\r\n                  [disabled]=\"saving || !profileForm.valid\">\r\n                  @if (saving) {\r\n                    <ion-spinner name=\"crescent\"></ion-spinner>\r\n                  } @else {\r\n                    <ion-icon name=\"save\" slot=\"start\"></ion-icon>\r\n                    Save Changes\r\n                  }\r\n                </ion-button>\r\n\r\n                <ion-button \r\n                  expand=\"block\" \r\n                  fill=\"outline\" \r\n                  (click)=\"toggleEditMode()\"\r\n                  [disabled]=\"saving\">\r\n                  Cancel\r\n                </ion-button>\r\n              </div>\r\n            </form>\r\n          }\r\n        </ion-card-content>\r\n      </ion-card>\r\n\r\n      <!-- Actions -->\r\n      @if (!editMode) {\r\n        <div class=\"actions\">\r\n          <ion-button expand=\"block\" color=\"danger\" (click)=\"signOut()\">\r\n            <ion-icon name=\"log-out\" slot=\"start\"></ion-icon>\r\n            Sign Out\r\n          </ion-button>\r\n        </div>\r\n      }\r\n    </div>\r\n  }\r\n</ion-content>\r\n"
        }
    ]
}