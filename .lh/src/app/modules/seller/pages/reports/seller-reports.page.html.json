            "name": "Commit-0",
            "content": "<ion-header [translucent]=\"true\">\r\n  <ion-toolbar color=\"warning\">\r\n    <ion-buttons slot=\"start\">\r\n      <ion-back-button defaultHref=\"/seller/dashboard\"></ion-back-button>\r\n    </ion-buttons>\r\n    <ion-title>My Reports</ion-title>\r\n    <ion-buttons slot=\"end\">\r\n      <ion-button (click)=\"refreshReports()\">\r\n        <ion-icon name=\"refresh\"></ion-icon>\r\n      </ion-button>\r\n    </ion-buttons>\r\n  </ion-toolbar>\r\n</ion-header>\r\n\r\n<ion-content [fullscreen]=\"true\">\r\n  <div class=\"report-container\" #reportContent>\r\n    @if (loading) {\r\n      <div class=\"loading-container\">\r\n        <ion-spinner name=\"crescent\"></ion-spinner>\r\n        <p>Loading reports...</p>\r\n      </div>\r\n    } @else {\r\n      <!-- Actions -->\r\n      <ion-card>\r\n        <ion-card-content>\r\n          <ion-button expand=\"block\" (click)=\"exportPDF()\">\r\n            <ion-icon name=\"download\" slot=\"start\"></ion-icon>\r\n            Export to PDF\r\n          </ion-button>\r\n        </ion-card-content>\r\n      </ion-card>\r\n\r\n      <!-- Stats -->\r\n      <ion-grid>\r\n        <ion-row>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"cash\" color=\"success\"></ion-icon>\r\n                <h2>â‚±{{ stats.totalSales.toFixed(2) }}</h2>\r\n                <p>Total Sales</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"receipt\" color=\"primary\"></ion-icon>\r\n                <h2>{{ stats.totalOrders }}</h2>\r\n                <p>Total Orders</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"cube\" color=\"warning\"></ion-icon>\r\n                <h2>{{ stats.totalProducts }}</h2>\r\n                <p>Products</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"checkmark-circle\" color=\"success\"></ion-icon>\r\n                <h2>{{ stats.completedOrders }}</h2>\r\n                <p>Completed</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n        </ion-row>\r\n      </ion-grid>\r\n\r\n      <!-- Charts -->\r\n      <ion-grid>\r\n        <ion-row>\r\n          <ion-col size=\"12\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Sales Over Time</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #salesChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n\r\n          <ion-col size=\"12\" sizeMd=\"6\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Top Products</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #productChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n\r\n          <ion-col size=\"12\" sizeMd=\"6\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Order Status</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #statusChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n        </ion-row>\r\n      </ion-grid>\r\n    }\r\n  </div>\r\n</ion-content>\r\n"
