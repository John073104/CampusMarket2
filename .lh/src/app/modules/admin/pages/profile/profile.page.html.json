            "date": 1763885282739,
            "name": "Commit-0",
            "content": "<ion-header [translucent]=\"true\">\r\n  <ion-toolbar color=\"danger\">\r\n    <ion-buttons slot=\"start\">\r\n      <ion-back-button defaultHref=\"/admin/dashboard\"></ion-back-button>\r\n    </ion-buttons>\r\n    <ion-title>Admin Profile</ion-title>\r\n    <ion-buttons slot=\"end\">\r\n      @if (!editing) {\r\n        <ion-button (click)=\"toggleEdit()\">\r\n          <ion-icon name=\"create\"></ion-icon>\r\n        </ion-button>\r\n      }\r\n    </ion-buttons>\r\n  </ion-toolbar>\r\n</ion-header>\r\n\r\n<ion-content [fullscreen]=\"true\">\r\n  @if (user) {\r\n    <div class=\"profile-container\">\r\n      <div class=\"profile-header\">\r\n        <ion-avatar class=\"large-avatar\">\r\n          @if (user.profileImage) {\r\n            <img [src]=\"user.profileImage\" [alt]=\"user.name\" (error)=\"onImageError($event)\" />\r\n          } @else {\r\n            <div class=\"avatar-placeholder\">\r\n              <ion-icon name=\"shield-checkmark\" size=\"large\"></ion-icon>\r\n            </div>\r\n          }\r\n        </ion-avatar>\r\n        <h2>{{ user.name || 'Administrator' }}</h2>\r\n        <p>{{ user.email }}</p>\r\n        <ion-badge color=\"danger\">\r\n          <ion-icon name=\"star\" size=\"small\"></ion-icon>\r\n          ADMIN\r\n        </ion-badge>\r\n      </div>\r\n\r\n      <ion-card>\r\n        <ion-card-header>\r\n          <ion-card-title>Account Information</ion-card-title>\r\n        </ion-card-header>\r\n\r\n        <ion-card-content>\r\n          <ion-list>\r\n            <ion-item>\r\n              <ion-label position=\"stacked\">Full Name</ion-label>\r\n              <ion-input \r\n                [(ngModel)]=\"user.name\"\r\n                [readonly]=\"!editing\"\r\n                type=\"text\"\r\n                placeholder=\"Enter your name\">\r\n              </ion-input>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-label position=\"stacked\">Email</ion-label>\r\n              <ion-input \r\n                [value]=\"user.email\"\r\n                readonly\r\n                type=\"email\">\r\n              </ion-input>\r\n              <ion-note slot=\"helper\">Email cannot be changed</ion-note>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-label position=\"stacked\">Phone Number</ion-label>\r\n              <ion-input \r\n                [(ngModel)]=\"user.phone\"\r\n                [readonly]=\"!editing\"\r\n                type=\"tel\"\r\n                placeholder=\"Enter phone number\">\r\n              </ion-input>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-label position=\"stacked\">Role</ion-label>\r\n              <ion-input \r\n                value=\"Administrator\"\r\n                readonly>\r\n              </ion-input>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-label position=\"stacked\">Member Since</ion-label>\r\n              <ion-input \r\n                [value]=\"user.dateJoined?.toDate?.() | date:'medium'\"\r\n                readonly>\r\n              </ion-input>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-label position=\"stacked\">Account Status</ion-label>\r\n              <ion-badge [color]=\"user.isActive ? 'success' : 'danger'\" slot=\"end\">\r\n                {{ user.isActive ? 'Active' : 'Inactive' }}\r\n              </ion-badge>\r\n            </ion-item>\r\n          </ion-list>\r\n\r\n          @if (editing) {\r\n            <div class=\"edit-actions\">\r\n              <ion-button expand=\"block\" (click)=\"saveProfile()\" [disabled]=\"loading\">\r\n                @if (loading) {\r\n                  <ion-spinner name=\"crescent\"></ion-spinner>\r\n                } @else {\r\n                  <ion-icon name=\"save\" slot=\"start\"></ion-icon>\r\n                  Save Changes\r\n                }\r\n              </ion-button>\r\n              <ion-button expand=\"block\" fill=\"outline\" (click)=\"toggleEdit()\">\r\n                <ion-icon name=\"close\" slot=\"start\"></ion-icon>\r\n                Cancel\r\n              </ion-button>\r\n            </div>\r\n          }\r\n        </ion-card-content>\r\n      </ion-card>\r\n\r\n      <!-- Admin Privileges Card -->\r\n      <ion-card>\r\n        <ion-card-header>\r\n          <ion-card-title>Administrator Privileges</ion-card-title>\r\n        </ion-card-header>\r\n\r\n        <ion-card-content>\r\n          <ion-list>\r\n            <ion-item>\r\n              <ion-icon name=\"people\" slot=\"start\" color=\"primary\"></ion-icon>\r\n              <ion-label>\r\n                <h3>User Management</h3>\r\n                <p>Full access to manage all users</p>\r\n              </ion-label>\r\n              <ion-icon name=\"checkmark-circle\" slot=\"end\" color=\"success\"></ion-icon>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-icon name=\"cube\" slot=\"start\" color=\"warning\"></ion-icon>\r\n              <ion-label>\r\n                <h3>Product Approval</h3>\r\n                <p>Review and approve product listings</p>\r\n              </ion-label>\r\n              <ion-icon name=\"checkmark-circle\" slot=\"end\" color=\"success\"></ion-icon>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-icon name=\"document-text\" slot=\"start\" color=\"tertiary\"></ion-icon>\r\n              <ion-label>\r\n                <h3>Seller Applications</h3>\r\n                <p>Review seller registration requests</p>\r\n              </ion-label>\r\n              <ion-icon name=\"checkmark-circle\" slot=\"end\" color=\"success\"></ion-icon>\r\n            </ion-item>\r\n\r\n            <ion-item>\r\n              <ion-icon name=\"bar-chart\" slot=\"start\" color=\"danger\"></ion-icon>\r\n              <ion-label>\r\n                <h3>Analytics & Reports</h3>\r\n                <p>Access to platform analytics</p>\r\n              </ion-label>\r\n              <ion-icon name=\"checkmark-circle\" slot=\"end\" color=\"success\"></ion-icon>\r\n            </ion-item>\r\n\r\n            <ion-item lines=\"none\">\r\n              <ion-icon name=\"settings\" slot=\"start\" color=\"secondary\"></ion-icon>\r\n              <ion-label>\r\n                <h3>System Configuration</h3>\r\n                <p>Manage system settings</p>\r\n              </ion-label>\r\n              <ion-icon name=\"checkmark-circle\" slot=\"end\" color=\"success\"></ion-icon>\r\n            </ion-item>\r\n          </ion-list>\r\n        </ion-card-content>\r\n      </ion-card>\r\n\r\n      <!-- Actions Card -->\r\n      <ion-card>\r\n        <ion-card-content>\r\n          <ion-button expand=\"block\" fill=\"outline\" color=\"danger\" (click)=\"logout()\">\r\n            <ion-icon name=\"log-out\" slot=\"start\"></ion-icon>\r\n            Sign Out\r\n          </ion-button>\r\n        </ion-card-content>\r\n      </ion-card>\r\n    </div>\r\n  }\r\n</ion-content>\r\n"
        }
    ]
}