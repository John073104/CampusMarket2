            "date": 1763388132036,
            "name": "Commit-0",
            "content": "<ion-header [translucent]=\"true\">\r\n  <ion-toolbar color=\"warning\">\r\n    <ion-buttons slot=\"start\">\r\n      <ion-back-button defaultHref=\"/admin/dashboard\"></ion-back-button>\r\n    </ion-buttons>\r\n    <ion-title>Reports & Analytics</ion-title>\r\n    <ion-buttons slot=\"end\">\r\n      <ion-button (click)=\"refreshReports()\">\r\n        <ion-icon name=\"refresh\"></ion-icon>\r\n      </ion-button>\r\n    </ion-buttons>\r\n  </ion-toolbar>\r\n</ion-header>\r\n\r\n<ion-content [fullscreen]=\"true\">\r\n  <div class=\"report-container\" #reportContent>\r\n    @if (loading) {\r\n      <div class=\"loading-container\">\r\n        <ion-spinner name=\"crescent\"></ion-spinner>\r\n        <p>Loading reports...</p>\r\n      </div>\r\n    } @else {\r\n      <!-- Date Range Filter -->\r\n      <ion-card>\r\n        <ion-card-content>\r\n          <ion-grid>\r\n            <ion-row>\r\n              <ion-col size=\"12\" sizeMd=\"5\">\r\n                <ion-item>\r\n                  <ion-label position=\"stacked\">From</ion-label>\r\n                  <ion-input \r\n                    type=\"date\" \r\n                    [(ngModel)]=\"dateRange.start\"\r\n                    (ionChange)=\"refreshReports()\">\r\n                  </ion-input>\r\n                </ion-item>\r\n              </ion-col>\r\n              <ion-col size=\"12\" sizeMd=\"5\">\r\n                <ion-item>\r\n                  <ion-label position=\"stacked\">To</ion-label>\r\n                  <ion-input \r\n                    type=\"date\" \r\n                    [(ngModel)]=\"dateRange.end\"\r\n                    (ionChange)=\"refreshReports()\">\r\n                  </ion-input>\r\n                </ion-item>\r\n              </ion-col>\r\n              <ion-col size=\"12\" sizeMd=\"2\" class=\"ion-text-center\">\r\n                <ion-button expand=\"block\" (click)=\"exportPDF()\">\r\n                  <ion-icon name=\"download\" slot=\"start\"></ion-icon>\r\n                  PDF\r\n                </ion-button>\r\n              </ion-col>\r\n            </ion-row>\r\n          </ion-grid>\r\n        </ion-card-content>\r\n      </ion-card>\r\n\r\n      <!-- Stats Cards -->\r\n      <ion-grid>\r\n        <ion-row>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"cash\" color=\"success\"></ion-icon>\r\n                <h2>â‚±{{ stats.totalSales.toFixed(2) }}</h2>\r\n                <p>Total Sales</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"receipt\" color=\"primary\"></ion-icon>\r\n                <h2>{{ stats.totalOrders }}</h2>\r\n                <p>Total Orders</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"cube\" color=\"warning\"></ion-icon>\r\n                <h2>{{ stats.totalProducts }}</h2>\r\n                <p>Products</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n          <ion-col size=\"6\" sizeMd=\"3\">\r\n            <ion-card class=\"stat-card\">\r\n              <ion-card-content>\r\n                <ion-icon name=\"people\" color=\"secondary\"></ion-icon>\r\n                <h2>{{ stats.totalUsers }}</h2>\r\n                <p>Users</p>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n        </ion-row>\r\n      </ion-grid>\r\n\r\n      <!-- Charts -->\r\n      <ion-grid>\r\n        <ion-row>\r\n          <!-- Sales Chart -->\r\n          <ion-col size=\"12\" sizeMd=\"6\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Sales Over Time</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #salesChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n\r\n          <!-- Category Chart -->\r\n          <ion-col size=\"12\" sizeMd=\"6\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Products by Category</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #categoryChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n\r\n          <!-- Top Sellers Chart -->\r\n          <ion-col size=\"12\" sizeMd=\"6\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Top 10 Sellers</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #topSellersChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n\r\n          <!-- Status Chart -->\r\n          <ion-col size=\"12\" sizeMd=\"6\">\r\n            <ion-card>\r\n              <ion-card-header>\r\n                <ion-card-title>Order Status Breakdown</ion-card-title>\r\n              </ion-card-header>\r\n              <ion-card-content>\r\n                <div class=\"chart-container\">\r\n                  <canvas #statusChart></canvas>\r\n                </div>\r\n              </ion-card-content>\r\n            </ion-card>\r\n          </ion-col>\r\n        </ion-row>\r\n      </ion-grid>\r\n\r\n      <!-- Order Status Details -->\r\n      <ion-card>\r\n        <ion-card-header>\r\n          <ion-card-title>Order Status Summary</ion-card-title>\r\n        </ion-card-header>\r\n        <ion-card-content>\r\n          <ion-list>\r\n            <ion-item>\r\n              <ion-icon name=\"time\" color=\"warning\" slot=\"start\"></ion-icon>\r\n              <ion-label>\r\n                <h3>Pending Orders</h3>\r\n                <p>Orders awaiting confirmation</p>\r\n              </ion-label>\r\n              <ion-badge color=\"warning\" slot=\"end\">{{ stats.pendingOrders }}</ion-badge>\r\n            </ion-item>\r\n            <ion-item>\r\n              <ion-icon name=\"checkmark-circle\" color=\"success\" slot=\"start\"></ion-icon>\r\n              <ion-label>\r\n                <h3>Completed Orders</h3>\r\n                <p>Successfully delivered</p>\r\n              </ion-label>\r\n              <ion-badge color=\"success\" slot=\"end\">{{ stats.completedOrders }}</ion-badge>\r\n            </ion-item>\r\n            <ion-item>\r\n              <ion-icon name=\"close-circle\" color=\"danger\" slot=\"start\"></ion-icon>\r\n              <ion-label>\r\n                <h3>Cancelled Orders</h3>\r\n                <p>Cancelled by customer or seller</p>\r\n              </ion-label>\r\n              <ion-badge color=\"danger\" slot=\"end\">{{ stats.cancelledOrders }}</ion-badge>\r\n            </ion-item>\r\n          </ion-list>\r\n        </ion-card-content>\r\n      </ion-card>\r\n    }\r\n  </div>\r\n</ion-content>\r\n"
        }
    ]
